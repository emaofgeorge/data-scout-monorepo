{
  "name": "shared-types",
  "root": "libs/shared-types",
  "sourceRoot": "libs/shared-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared-types",
        "main": "libs/shared-types/src/index.ts",
        "tsConfig": "libs/shared-types/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/shared-types/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/shared-types"],
      "options": {
        "jestConfig": "libs/shared-types/jest.config.cts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
