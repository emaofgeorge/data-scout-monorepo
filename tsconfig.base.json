{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data-scout/core-ikea": ["dist/libs/core-ikea/src/index.d.ts"],
      "@data-scout/core-ikea/*": ["dist/libs/core-ikea/src/*"]
    },
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "esModuleInterop": true
  }
}
