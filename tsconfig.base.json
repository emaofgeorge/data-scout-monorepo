{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data-scout/core-ikea": ["libs/core-ikea/src/index.ts"],
      "@data-scout/shared-types": ["libs/shared-types/src/index.ts"],
      "@data-scout/core-scraper": ["libs/core-scraper/src/index.ts"],
      "@data-scout/core-storage": ["libs/core-storage/src/index.ts"],
      "@data-scout/core-notifications": ["libs/core-notifications/src/index.ts"]
    },
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "esModuleInterop": true
  }
}
