{
  "name": "@data-scout/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve ikea-circularity-scraper",
    "start:dev": "nx serve ikea-circularity-scraper --watch",
    "start:scraper": "nx serve ikea-circularity-scraper",
    "start:bot": "nx serve ikea-circularity-bot",
    "start:webhook": "nx serve ikea-circularity-bot --configuration=webhook",
    "build": "nx run-many -t build",
    "build:libs": "nx run-many -t build --projects=shared-types,core-scraper,core-storage,core-notifications,core-ikea",
    "build:scraper": "nx build ikea-circularity-scraper",
    "build:bot": "nx build ikea-circularity-bot",
    "test": "nx run-many -t test",
    "test:ikea": "nx run-many -t test --projects=ikea-circularity-scraper,ikea-circularity-bot",
    "test:watch": "nx run-many -t test --projects=ikea-circularity-scraper,ikea-circularity-bot --watch",
    "lint": "nx run-many -t lint",
    "lint:ikea": "nx run-many -t lint --projects=ikea-circularity-scraper,ikea-circularity-bot",
    "lint:fix": "nx run-many -t lint --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "graph": "nx graph",
    "clean": "nx reset",
    "emulator:start": "firebase emulators:start --project demo-ikea-circularity",
    "emulator:export": "firebase emulators:start --project demo-ikea-circularity --export-on-exit=./firebase-data",
    "emulator:import": "firebase emulators:start --project demo-ikea-circularity --import=./firebase-data",
    "sync": "nx sync"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "22.1.3",
    "@nx/eslint": "22.1.3",
    "@nx/eslint-plugin": "22.1.3",
    "@nx/jest": "22.1.3",
    "@nx/js": "22.1.3",
    "@nx/node": "^22.1.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/user-agents": "^1.0.4",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "firebase-tools": "^14.27.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "nx": "22.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "firebase-admin": "^13.6.0",
    "telegraf": "^4.16.3",
    "user-agents": "^1.1.669"
  }
}
